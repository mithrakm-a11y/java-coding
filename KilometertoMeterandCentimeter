import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

      
        int A = sc.nextInt();

   
        int B = A * 1000;

    
        int C = A * 100000;

        System.out.println(B);
        System.out.println(C);

        sc.close();
    }
}
